<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #D0DDF2;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="welcomeLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="125.0" layoutY="101.0" text="Welcome" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Label alignment="CENTER" layoutY="209.0" text="Click the button below for redirect." AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <StackPane layoutY="238.0" prefHeight="63.0" prefWidth="113.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="goToButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#goToHome" style="-fx-background-color: #8EA1BF; -fx-border-color: #283955; -fx-border-radius: 7; -fx-background-radius: 11;" text="Go to Home page" />
         </children>
      </StackPane>
   </children>
</AnchorPane>
